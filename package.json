{
  "private": false,
  "name": "partygame-server",
  "version": "2.0.1",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts | pino-pretty",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "build": "npm run clean && tsc",
    "clean": "node node_modules/rimraf/bin dist",
    "test": "mocha --require ts-node/register test/**_test.ts --exit --timeout 15000"
  },
  "license": "AGPL-3.0",
  "author": "Jakub Kopa≈Ñko <hi@kopanko.com> (kopanko.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/pcktm/partygame-server"
  },
  "devDependencies": {
    "@colyseus/loadtest": "^0.14.4",
    "@colyseus/testing": "^0.14.21",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^8.2.3",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^9.0.2",
    "pino-pretty": "^9.0.0",
    "prisma": "^4.3.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.14.20",
    "@colyseus/schema": "^1.0.36",
    "@colyseus/ws-transport": "^0.14.21",
    "@prisma/client": "^4.3.1",
    "colyseus": "^0.14.20",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-vault": "^1.11.2",
    "express": "^4.16.4",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pino": "^8.4.1",
    "pino-http": "^8.2.0",
    "ua-parser-js": "^1.0.2"
  }
}
